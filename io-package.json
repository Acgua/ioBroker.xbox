        "news": {
            "0.5.3": {
                "en": "improve log message quality, more promisification",
                "de": "Verbesserung der Protokollnachrichtenqualität, mehr Promisifications",
                "ru": "улучшить качество сообщений журнала, больше обещаний",
                "pt": "melhorar a qualidade da mensagem de log, mais promisification",
                "nl": "verbetering van de kwaliteit van het logbericht, meer belofte",
                "fr": "améliorer la qualité des messages de log, plus de promisification",
                "it": "migliorare la qualità dei messaggi di registro, maggiore promessa",
                "es": "mejorar la calidad de los mensajes de registro, más promisificación",
                "pl": "popraw jakość logu, więcej promisyfikacji"
            },
            "0.5.0": {
                "en": "support of compact mode and small fixes and optimizations",
                "de": "Unterstützung des Kompaktmodus und kleiner Korrekturen und Optimierungen",
                "ru": "поддержка компактного режима и небольших исправлений и оптимизаций",
                "pt": "suporte de modo compacto e pequenas correções e otimizações",
                "nl": "ondersteuning van compacte modus en kleine fixes en optimalisaties",
                "fr": "support du mode compact et de petites corrections et optimisations",
                "it": "supporto della modalità compatta e piccole correzioni e ottimizzazioni",
                "es": "Soporte de modo compacto y pequeñas correcciones y optimizaciones.",
                "pl": "obsługa trybu kompaktowego oraz małych poprawek i optymalizacji"
            },
            "0.4.2": {
                "en": "use adapter-core",
                "de": "Nutzung von Adapter-Core"
            },
            "0.4.1": {
                "en": "Minor type fix",
                "de": "Minimaler Typ fix"
            },
            "0.4.0": {
                "en": "Seek state fixed to a number to jump to positions on media content, try reauth when losing auth",
                "de": "Seek state zu Number transformiert um zu einer bestimmten Stelle zu springen, versuche erneute Authentifizierung bei Verlust"
            },
            "0.3.0": {
                "en": "New state activeTitleType added, 2 factor auth fixed",
                "de": "Neuer State activeTitleType hinzugefügt, 2-Faktor Authentifizierung hinzugefügt"
            },
            "0.2.2": {
                "en": "Minor fix when currentTitles empty, activeTitle states should be too",
                "de": "Fix, dass in activeTitle states keine Angaben mehr stehen, wenn currenTitles leer ist"
            },
            "0.2.1": {
                "en": "minor fix on state name",
                "de": "Fix für State Name"
            },
            "0.2.0": {
                "en": "added authentication for xboxLive",
                "de": "Authentikation zu Xbox Live hinzugefügt"
            },
            "0.1.7": {
                "en": "Rest-Server will no be stopped at unload on windows too",
                "de": "Rest-Server wird nun auch unter Windows bei triggern von unload gestoppt"
            },
            "0.1.6": {
                "en": "fix start of rest server for windows when nopy not in own node_modules",
                "de": "Ergänzender Fix für das Starten des Rest Server unter Windows"
            },
            "0.1.5": {
                "en": "fix start command of rest server for windows and try to kill on windows by pid",
                "de": "Starten des Rest-Servers unter Windows gefixt sowie beenden des Servers"
            },
            "0.1.4": {
                "en": "set info.connection and settings.power to false on unload, not only rely on ping to check if xbox is on, use available too",
                "de": "info.connection und settings.power werden bei Adapter Stop nun sicher auf false gesetzt, Ping wird nun kombiniert mit Available State genutzt"
            },
            "0.1.3": {
                "en": "bump smartglass-rest to 0.9.7, powering on not multicastable consoles should work now, minor fix, reduce discovery",
                "de": "smartglass-rest library auf 0.9.7 erhöht, nicht per Multicast erreichbare Konsolen können nun auch eingeschaltet werden, kleine Fehlerbehebung, Discovery nur wenn Xbox online und nicht verbunden"
            },
            "0.1.2": {
                "en": "minor fix on currentTitles beeing empty",
                "de": "Fehlerbehebung um mit leerem currentTitles State umzugehen"
            },
            "0.1.1": {
                "en": "minor fixes, explicitly require python deps version, fix for power on when not accessable via broadcast",
                "de": "kleine Fehlerbehebungen, explizite Versionen Python Abhängigkeiten festgelegt, Power on gefixt, wenn Xbox nicht im Broadcast Netzwerk"
            },
            "0.1.0": {
                "en": "added possibility to enter text in active text fields, brought back live id to settings, find consoles which are not available via broadcast, title states added",
                "de": "State zur Texteingabe hinzugefügt, Live ID wurde zurück gebracht, Konsolen können nun auch gefunden werden, wenn sie nicht über Broadcast erreichbar sind, Titel Stati hinzugefügt"
            },
            "0.0.13": {
                "en": "minor fix, restart adapter on rest server error, log lose of connection without ping",
                "de": "kleine Fehlerbehebung, Adapter neustarten, wenn Rest Server Fehler, erkenne Verbindungsverlust auch ohne Ping"
            },
            "0.0.12": {
                "en": "do not connect when console unavailable, log unavailable console on debug, only set power states on change",
                "de": "Keine Verbindungsversuche mehr, wenn Konsole nicht verfügbar, Nicht-Verfügbarkeit wird auf Debug geloggt, Power Stati werden nur noch bei Änderung gesetzt"
            },
            "0.0.11": {
                "en": "minor connection fix",
                "de": "kleiner Verbindungsfix"
            },
            "0.0.10": {
                "en": "improve connection handling",
                "de": "Zuverlässigere Verbindungsherstellung"
            },
            "0.0.9": {
                "en": "remove live id as parameter",
                "de": "LiveID ist nicht mehr nötig"
            },
            "0.0.8": {
                "en": "Reduce spam of reconnect logging, fixes in io-package like title lang and removed mail, pw",
                "de": "Reconnect logging reduziert, kleinere Anpassungen in io-package.json"
            },
            "0.0.6": {
                "en": "Stop making connect requests when already connected, more user friendly logging",
                "de": "Keine Verbindungsversuche mehr wenn bereits verbunden, nutzerfreundlicheres Logging"
            },
            "0.0.5": {
                "en": "Start and stop REST server on windows, using relative paths",
                "de": "Starten und stoppen des REST Servers auf Windows, Nutzung relativer Pfade"
            },
            "0.0.4": {
                "en": "Automatically install required Debian packages, detect OS, improved logging, installation for Windows",
                "de": "Automatisches installieren der benötigten Debian Packages, Betriebssystem wird erkannt, besseres Logging, Windows Installation"
            },
            "0.0.3": {
                "en": "Fix endless power on tries, fix state changes for power off, use ping to check powered on",
                "de": "Es wird nicht mehr endlos versucht einzuschalten, power off geht wieder, ping wird genutzt für power status"
            },
            "0.0.2": {
                "en": "Automated installation of deps, improved Code, fixed endpoints",
                "de": "Abhängigkeiten werden nun automatisch isntalliert, Endpunkte korrigiert, Codeverbesserungen"
            },
            "0.0.1": {
                "en": "initial adapter",
                "de": "Initiale Version"
            }
        },
